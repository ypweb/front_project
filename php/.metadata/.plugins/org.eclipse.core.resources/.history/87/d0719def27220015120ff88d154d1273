<?php
 $a=null;
 $b;
 $c='ni mei';
 unset($c);
 var_dump($a,$b,$c);
 echo '<br />';
 echo '$_COOKIE: ';
 var_dump($_COOKIE);
 echo '<br />';
 echo '$_ENV: ';
 var_dump($_ENV);
 echo '<br />';
 echo '$_FILES: ';
 var_dump($_FILES);
 
 echo '$_GET: ';
 var_dump($_GET);
 
 echo '$_POST: ';
 var_dump($_POST);
 
 echo '$_REQUEST: ';
 var_dump($_REQUEST);
 
 echo '$_SERVER: ';
 var_dump($_SERVER);
 
 echo '$_SESSION: ';
 var_dump($_SESSION);
 
 echo '$GLOBALS: ';
 var_dump($GLOBALS);
 
?>











